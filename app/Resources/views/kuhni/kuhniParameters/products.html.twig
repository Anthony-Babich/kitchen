{% set container1 %}
    <div class="container">
        <div class="row">
            <div class="col-xl-6 col-md-12 big-col">
                {% for kitchen in kitchens %}
                    {% if loop.index0 == 0 %}
                        <a href="{{ path('kuhni_product', {'slug' : slug, 'nameproduct' : kitchen.slug}) }}">
                            <img class="slide-product-img big" src="{{ asset( image[loop.index0] ) }}" alt="{{ kitchen.keywords }}" title="{{ kitchen.title }}">
                            <span class="pos-bot-l" {% if kitchen.discount == 0 %}style="width: 100%;" {% endif %}>
                                <ul class="nav">
                                    <li class="left">
                                        <div class="text-left">
                                            <span class="first-name"><b>{{ kitchen.title }}</b><br/></span>
                                            <span class="first-desc">
                                                {% if kitchen.fixedPrice %}
                                                    *Цена указана за 1 метр погонный кухни
                                                {% else %}
                                                    *Цену и наличие уточняйте
                                                {% endif %}
                                            </span>
                                        </div>
                                    </li>
                                    <li class="right">
                                        <div class="text-right right">
                                            <span class="text-right last-price">старая цена
                                                <span class="through">{{ kitchen.noDiscountPrice }}</span><br/>
                                            </span>
                                            <span class="text-right now-price">сейчас от {{ kitchen.price }}</span>
                                        </div>
                                    </li>
                                </ul>
                            </span>
                            {% if kitchen.discount != 0 %}
                                <span class="pos-bot-r desc text-center">
                                    <span class="title"><b>{{ kitchen.discount }}%</b></span>
                                    <span>скидка</span>
                                </span>
                            {% endif %}
                        </a>
                        <button type="button" class="phone text-center" data-toggle="modal" data-target="#requestcall">
                            <i class="fa fa-phone"></i>
                        </button>
                        <button type="button" class="like text-center">
                            <i class="fa fa-heart"></i> {{ kitchen.likes }}
                        </button>
                    {% endif %}
                {% endfor %}
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-xl-6">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-6 small-col no-margin-left full-screen">
                        {% for kitchen in kitchens %}
                            {% if loop.index0 <= 2 and loop.index0 > 0 %}
                                <div class="col-12 big-col">
                                    <a href="{{ path('kuhni_product', {'slug' : slug, 'nameproduct' : kitchen.slug}) }}">
