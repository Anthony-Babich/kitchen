{% set container1 %}
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-xl-6 big-col">
                {% for project in projects %}
                    {% if loop.index0 == 0 %}
                        <a href="#{#{{ path('kuhni_product', {'slug' : slug, 'nameproduct' : project.slug}) }}#}">
                            <img class="slide-product-img big" src="{{ asset( imageProject[loop.index0] ) }}" alt="{{ project.keywords }}" title="{{ project.title }}">
                            <span class="pos-bot-l" {% if project.discount == 0 %}style="width: 100%;" {% endif %}>
                                <ul class="nav">
                                    <li class="left">
                                        <div class="text-left">
                                            <span class="first-name"><b>{{ project.title }}</b><br/></span>
                                        </div>
                                    </li>
                                    <li class="right">
                                        <div class="text-right right">
                                            {% set price = (project.price * kurs.setting * nds.setting * coef.setting)|round %}
                                            {% set noDiscountPrice = (((price * project.discount)/100 + price)|round)|number_format(0, ' ', ' ') %}
                                            {% if project.discount != 0 %}
                                                <span class="text-right last-price">старая цена
                                                    <span class="through">{{ noDiscountPrice }} <i class='fa fa-rub' aria-hidden='true'></i></span><br/>
                                                </span>
                                            {% endif %}
                                            <span class="text-right now-price">сейчас от {{ price|number_format(0, ' ', ' ') }} <i class='fa fa-rub' aria-hidden='true'></i></span>
                                        </div>
                                    </li>
                                </ul>
                            </span>
                            {% if project.discount != 0 %}
                                <span class="pos-bot-r desc text-center">
                                    <span class="title"><b>{{ project.discount }}%</b></span>
                                    <span>скидка</span>
                                </span>
                            {% endif %}
                        </a>
                        <button type="button" class="phone text-center" data-toggle="modal" data-target="#requestcall">
                            <i class="fa fa-phone"></i>
                        </button>
                        <button type="button" id="like" data-id="{{ project.id }}" class="like text-center">
                            <i class="fa fa-heart"></i>
                            <span class="countLikes">
                                {{ project.likes }}
                            </span>
                        </button>
                    {% endif %}
                {% endfor %}
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-xl-6">
